{
    "name": "sandermj/nova-impersonate",
    "description": "A Laravel Nova field allows you to authenticate as your users.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "lab404/laravel-impersonate": "^1.4.1"
    },
    "autoload": {
        "psr-4": {
            "sandermj\\NovaImpersonate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "sandermj\\NovaImpersonate\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
